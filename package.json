{
  "name": "svgson",
  "version": "3.0.0-rc.0",
  "description": "Transform svg files into Objects",
  "main": "dist/svgson.cjs.js",
  "module": "dist/svgson.esm.js",
  "browser": "dist/svgson.umd.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "ava",
    "pretest": "npm run build",
    "cover": "nyc --reporter=lcov --reporter=text npm t",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elrumordelaluz/svgson.git"
  },
  "keywords": [
    "svg",
    "json",
    "svgo"
  ],
  "author": {
    "name": "Lionel",
    "email": "elrumordelaluz@hotmail.com",
    "url": "http://elrumordelaluz.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elrumordelaluz/svgson/issues"
  },
  "homepage": "https://github.com/elrumordelaluz/svgson#readme",
  "dependencies": {
    "clean-deep": "3.0.2",
    "deep-rename-keys": "^0.2.1",
    "omit-deep": "0.3.0",
    "xml-reader": "2.4.3"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "ava": "0.25.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.2.0",
    "codecov.io": "0.1.6",
    "nyc": "13.1.0",
    "rollup": "0.67.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "svgo": "1.1.1",
    "svgson": "2.1.1"
  },
  "nyc": {
    "lines": 90,
    "statements": 90,
    "functions": 70,
    "branches": 70,
    "check-coverage": true,
    "exclude": [
      "**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "xo": {
    "esnext": true
  }
}
