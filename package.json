{
  "name": "svgson",
  "version": "5.2.1",
  "description": "Transform svg files into Objects",
  "main": "dist/svgson.cjs.js",
  "module": "dist/svgson.esm.js",
  "browser": "dist/svgson.umd.js",
  "types": "types.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "ava",
    "pretest": "npm run build",
    "cover": "nyc --reporter=lcov --reporter=text npm t",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elrumordelaluz/svgson.git"
  },
  "keywords": [
    "svg",
    "json",
    "svgo"
  ],
  "files": [
    "dist",
    "types.d.ts",
    "README.md"
  ],
  "author": {
    "name": "Lionel",
    "email": "elrumordelaluz@hotmail.com",
    "url": "http://elrumordelaluz.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elrumordelaluz/svgson/issues"
  },
  "homepage": "https://github.com/elrumordelaluz/svgson#readme",
  "dependencies": {
    "deep-rename-keys": "^0.2.1",
    "omit-deep": "0.3.0",
    "xml-reader": "2.4.3"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.14",
    "ava": "^3.15.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.3.4",
    "codecov": "^3.8.1",
    "lodash.transform": "^4.6.0",
    "nyc": "^15.1.0",
    "rollup": "^2.44.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "svgo": "^1.3.2"
  },
  "nyc": {
    "lines": 90,
    "statements": 90,
    "functions": 70,
    "branches": 70,
    "check-coverage": true,
    "exclude": [
      "**/*.test.js",
      "rollup.config.js",
      "test.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "xo": {
    "esnext": true
  },
  "ava": {
    "babel": true,
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  }
}
